{
    "name": "php-etl/array-expression-language",
    "description": "This library implements functions for manipulating array data in ExpressionLanguage",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "symfony/expression-language": "^6.0",
        "symfony/property-access": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.26.18",
        "rector/rector": "^0.15"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\ArrayExpressionLanguage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\ArrayExpressionLanguage\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
